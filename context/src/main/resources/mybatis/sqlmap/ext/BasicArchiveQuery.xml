<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zqkh.archive.context.appservice.impl.domain.repository.query.BasicArchiveQuery">
    <select id="selectBasicArchiveIdByMemberId" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT
        id
        FROM
        t_basic_archive
        where family_member_id = #{memberId}
    </select>
</mapper>